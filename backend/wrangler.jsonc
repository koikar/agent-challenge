    "SUPABASE_URL": "https://ywczcwrjrpjvqoetbbsh.supabase.co"
  },
  "ai": {
    "binding": "AI",
  },
  "r2_buckets": [
    {
      "binding": "BRAND_CONTENT_BUCKET",
      "bucket_name": "brands",
    },
  ],
  "name": "tedix-agent-challenge-mcp",
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
  },
  "triggers": {
    "crons": ["* * * * *"],
  },
